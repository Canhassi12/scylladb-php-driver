{
  "cmake.buildDirectory": "${workspaceFolder}/build/Debug",
  "cmake.allowCommentsInPresetsFile": true,
  "cmake.buildTask": true,
  "cmake.configureEnvironment": {
    "PHP_SCYLLADB_LIBUV_STATIC": "ON",
    "PHP_SCYLLADB_LIBSCYLLADB_STATIC": "ON",
    "CASS_CPP_STANDARD": "17",
    "CASS_USE_STATIC_LIBS": "ON"
  },
  "cmake.configureSettings": {},
  "cmake.generator": "Ninja",
  "cmake.configureOnOpen": true,
  "files.associations": {
    "*main.yml": "ansible",
    "php_driver.h": "c",
    "zend_exceptions.h": "c",
    "php.h": "c",
    "types.h": "c",
    "php_driver_globals.h": "c",
    "ref.h": "c",
    "php_driver_types.h": "c",
    "zend_smart_str.h": "c"
  }
}
