{
  "configurations": [
    {
      "name": "Linux",
      "includePath": [
        "${env:HOME}/php/8.2-nts/include/php",
        "${env:HOME}/php/8.2-nts/include/php/Zend",
        "${env:HOME}/php/8.2-nts/include/php/main",
        "${env:HOME}/php/8.2-nts/include/php/sapi",
        "${env:HOME}/php/8.2-nts/include/php/TSRM",
        "${workspaceFolder}/include",
        "${workspaceFolder}/build/Debug"
      ],
      "defines": ["UV_STATIC=1", "SCYLLA_STATIC=1", "HAVE_CONFIG_H"],
      "compilerPath": "/usr/bin/clang",
      "intelliSenseMode": "linux-clang-x64",
      "cStandard": "c17",
      "cppStandard": "c++20"
    }
  ],
  "version": 4
}
